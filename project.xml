<?xml version="1.0" encoding="UTF-8"?>

<project xmlns:j="jelly:core">
  <extend>${base.project.dir}/base-dependencies.xml</extend>
  <name>idegaWeb Cluster</name>
  <artifactId>com.idega.cluster</artifactId>
  <currentVersion>3.1.0</currentVersion>
  <inceptionYear>2006</inceptionYear>
  <shortDescription>add on for clustering</shortDescription>
  <issueTrackingUrl>http://issues.idega.is</issueTrackingUrl>
  <versions>
    <version>
      <id>3.1.0</id>
      <name>3.1.0</name>
      <tag>CLUSTER_3_1_0</tag>
    </version>
  </versions>
  <branches/>
  <developers>
    <developer>
      <id>thomas</id>
      <name>Thomas Hilbig</name>
      <email>thomas@idega.is</email>
      <organization>idega software</organization>
      <roles/>
    </developer>
  </developers>
  <dependencies>
    <dependency>
      <groupId>bundles</groupId>
      <artifactId>com.idega.core</artifactId>
      <version>3.1.94</version>
      <type>jar</type>
      <url>http://www.idega.com/</url>
      <properties>
        <eclipse.dependency>true</eclipse.dependency>
      </properties>
    </dependency>
    <dependency>
      <groupId>ehcache</groupId>
      <artifactId>ehcache</artifactId>
      <version>1.2</version>
      <type>jar</type>
      <url>http://ehcache.sourceforge.net/</url>
      <properties>
        <!-- already included in core bundle -->

        <idegaweb.bundle.include>false</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>bundles</groupId>
      <artifactId>net.jxta</artifactId>
      <version>1.0</version>
      <type>jar</type>
      <url>http://www.idega.com/</url>
      <properties>
        <eclipse.dependency>true</eclipse.dependency>
      </properties>
    </dependency>
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.8</version>
      <type>jar</type>
      <url>http://logging.apache.org/log4j/</url>
      <properties>
        <idegaweb.bundle.include>false</idegaweb.bundle.include>
      </properties>
    </dependency>
  </dependencies>
</project>
